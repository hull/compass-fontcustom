@import "fontcustom.scss";

$<%= name %>-font-base-class: ".<%= name %>-font" !default;

#{$<%= name %>-font-base-class} {
	font-family: "<%= name %>";
}

@mixin all-<%= name %>-letters {
  @include fontcustom-font-face(<%= name %>);
  @include letters(<%= name %>, <%= letter_names.join(" ") %>, $<%= name %>-font-base-class)
}
